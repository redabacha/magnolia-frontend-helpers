{
  "env": {
    "browser": true,
    "es6": true,
    "jasmine": true,
    "jest": true
  },
  "extends": ["eslint:recommended", "airbnb-base/legacy", "prettier"],
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly",
    "process": "readonly",
    "module": "readonly",
    "require": "readonly",
    "__dirname": "readonly"
  },
  "parserOptions": {
    "ecmaVersion": 2020,
    "sourceType": "module"
  },
  "rules": {
    "no-unused-vars": "warn",
    "no-use-before-define": "off",
    "no-nested-ternary": "off"
  }
}
